<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.ImageInput?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="300.0" style="-fx-background-color: #fae100;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.SettingsPageController">
   <children>
      <Button fx:id="musicon" layoutX="79.0" layoutY="153.0" mnemonicParsing="false" onAction="#VolumeOn" prefHeight="49.0" prefWidth="46.0" text="Button">
         <effect>
            <ImageInput>
               <source>
                  <Image url="@../sample/Assets/musicon.png" />
               </source>
            </ImageInput>
         </effect></Button>
      <Button fx:id="tick" layoutX="109.0" layoutY="253.0" mnemonicParsing="false" onAction="#closeSettings" prefHeight="83.0" prefWidth="82.0" text="Button">
         <effect>
            <ImageInput>
               <source>
                  <Image url="@../sample/Assets/check.png" />
               </source>
            </ImageInput>
         </effect>
      </Button>
      <Button fx:id="musicoff" layoutX="174.0" layoutY="153.0" mnemonicParsing="false" onAction="#VolumeOff" prefHeight="50.0" prefWidth="46.0" text="Button">
         <effect>
            <ImageInput>
               <source>
                  <Image url="@../sample/Assets/musicoff.png" />
               </source>
            </ImageInput>
         </effect>
      </Button>
   </children>
</AnchorPane>
