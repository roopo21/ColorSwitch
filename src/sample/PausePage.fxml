<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.ImageInput?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="300.0" style="-fx-background-color: #900dff;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.PausePageController">
    <children>
        <Button fx:id="restart" layoutX="92.0" layoutY="155.0" mnemonicParsing="false" prefHeight="34.0" prefWidth="114.0" style="-fx-background-color: #272727;" text="RESTART" textFill="WHITE">
            <font>
                <Font name="Ebrima Bold" size="17.0" />
            </font>
        </Button>
        <Button fx:id="endgame" layoutX="93.0" layoutY="215.0" mnemonicParsing="false" style="-fx-background-color: #272727;" text="END GAME" textFill="WHITE">
            <font>
                <Font name="Ebrima Bold" size="17.0" />
            </font>
        </Button>
        <Button fx:id="savegame" layoutX="93.0" layoutY="284.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="50.0" text="Button">
            <effect>
                <ImageInput>
                    <source>
                        <Image url="@../sample/Assets/flatDark33.png" />
                    </source>
                </ImageInput>


            </effect>
        </Button>
        <Button fx:id="settings" layoutX="156.0" layoutY="284.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="50.0" text="Button">
            <effect>
                <ImageInput>
                    <source>
                        <Image url="@../sample/Assets/shadedDark31.png" />
                    </source>
                </ImageInput>


            </effect>

        </Button>
        <Button fx:id="closebutton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#changeScreenRemovesettings" prefHeight="50.0" prefWidth="50.0" text="Button">
            <effect>
                <ImageInput>
                    <source>
                        <Image url="@../sample/Assets/close.png" />
                    </source>
                </ImageInput>


            </effect>

        </Button>
    </children>


</AnchorPane>
