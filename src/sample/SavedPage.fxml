<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="720.0" prefWidth="1345.0" style="-fx-background-color: #272727;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.SavedPageController">
   <children>
      <Text fill="#545454" layoutX="33.0" layoutY="124.0" strokeType="OUTSIDE" strokeWidth="0.0" text="  SELECT A GAME TO CONTINUE">
         <font>
            <Font name="Franklin Gothic Medium" size="95.0" />
         </font>
      </Text>
      <Button fx:id="back" layoutX="629.0" layoutY="644.0" mnemonicParsing="false" onAction="#changeScreenBacktoMain" prefHeight="46.0" prefWidth="87.0" style="-fx-background-color: #FAE100;" text="BACK">
         <font>
            <Font name="Franklin Gothic Medium" size="20.0" />
         </font></Button>
      <ListView fx:id="gameStateList" layoutX="571.0" layoutY="278.0" onMouseClicked="#handleMouseClick" prefHeight="200.0" prefWidth="200.0" />
      <Button fx:id="deleteProgress" layoutX="629.0" layoutY="586.0" mnemonicParsing="false" onAction="#deleteAllProgress" prefHeight="46.0" prefWidth="87.0" style="-fx-background-color: #FAE100;" text="Delete">
         <font>
            <Font name="Franklin Gothic Medium" size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
